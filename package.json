{
  "name": "arcade",
  "displayName": "Arcade",
  "description": "Vscode Extension for interacting with hackhour api",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "arcade.Time",
        "title": "Arcade: Time"
        
      },
      {
        "command": "arcade.Start",
        "title": "Arcade: Start"
      },
      {
        "command": "arcade.Stop",
        "title": "Arcade: Stop"
      },
      {
        "command": "arcade.Pause",
        "title": "Arcade: Pause"
      },
      {
        "command": "arcade.Test",
        "title": "Arcade: Test"
      },
      {
        "command": "arcade.Setup",
        "title": "Arcade: Setup"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  }
  
}
