{
  "name": "arcade-hackhour",
  "displayName": "Arcade Hackhour",
  "publisher": "Outdatedcandy92",
  "description": "Vscode Extension for interacting with hackhour api",
  "version": "0.2.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "icon": "icon.png",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Outdatedcandy92/Arcade-VSCode-Extension"
  },
  "activationEvents": [
    "onStartupFinished",
    "onCommand:arcade-hackhour.setup",
    "onCommand:arcade-hackhour.test"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "arcade-hackhour.time",
        "title": "Arcade: Time"
      },
      {
        "command": "arcade-hackhour.start",
        "title": "Arcade: Start"
      },
      {
        "command": "arcade-hackhour.stop",
        "title": "Arcade: Stop"
      },
      {
        "command": "arcade-hackhour.pause",
        "title": "Arcade: Pause"
      },
      {
        "command": "arcade-hackhour.test",
        "title": "Arcade: Test"
      },
      {
        "command": "arcade-hackhour.setup",
        "title": "Arcade: Setup"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "fetch": "^1.1.0",
    "node-fetch": "^3.3.2"
  }
}
